{
  "name": "igithub-sentry-deployment-gate",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/getsentry/github-deployment-gate.git"
  },
  "engines": {
    "node": "16.x"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts && prettier -c src",
    "lint:ci": "npm run lint",
    "lint:dev": "nodemon -e 'ts' -x 'npm run lint'",
    "lint:fix": "eslint . --ext .ts --fix && prettier -c src --write",
    "test": "NODE_ENV=test jest -i .spec.ts",
    "test:ci": "npm run test",
    "test:dev": "npm run test -- --watch",
    "build": "rimraf dist; tsc"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "octokit": "^2.0.14",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.29.3",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "dotenv": "^16.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.5.1",
    "mocha": "^9.2.1",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "rimraf": "^4.4.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.9.5"
  }
}
